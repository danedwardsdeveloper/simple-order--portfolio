{
	"type": "module",
	"scripts": {
		"dev": "next dev --turbo",
		"email": "email dev --dir ./src/library/email/templates/definitions",
		"start": "node .next/standalone/server.js",
		"format": "biome format . --write",
		"check": "biome check . --write --unsafe",
		"stripe": "stripe listen --events customer.subscription.created,customer.subscription.updated,invoice.paid,invoice.payment_failed,customer.subscription.deleted --forward-to localhost:3000/api/payments/webhook",
		"portfolio-sync": "rsync -av --exclude='.git' --exclude='.gitignore' --exclude='node_modules' --exclude='.vercel' --exclude='.next' --exclude='drizzle/' --exclude='.env*' --exclude='src/app/api/authentication/' --exclude='src/app/api/orders/' --exclude='src/app/api/payments/' --exclude='src/library/database/' --exclude='src/library/email/' --exclude='public/images/' --exclude='src/library/stripe/' --exclude='src/library/utilities/server/' --exclude='src/library/validations/' --exclude='src/types/' ./ ../simple-order--portfolio/",
		"build": "pnpm format && pnpm check && next build",
		"seed": "pnpm tsx src/library/database/seed",
		"seed:production": "NODE_ENV=production pnpm tsx src/library/database/seed",
		"generate": "drizzle-kit generate --config=drizzle.config.ts",
		"push": "drizzle-kit push --config=drizzle.config.ts",
		"push:production": "drizzle-kit push --config=drizzle.production.config.ts",
		"studio": "npx drizzle-kit studio --config=drizzle.config.ts",
		"studio:production": "npx drizzle-kit studio --config=drizzle.production.config.ts",
		"test": "vitest",
		"lint": "next lint",
		"predeploy": "pnpm push:production && pnpm seed:production&& pnpm build",
		"deploy": "vercel --prod"
	},
	"dependencies": {
		"@headlessui/react": "2.2.0",
